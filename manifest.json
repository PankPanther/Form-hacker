{
  "name": "Form Hacker",
  "description": "Display's all the hidden inputs in form.",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "tabs"
  ],

  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icons/icon-96.png",
    "default_title": "Form Hacker"
  },
  "manifest_version": 2
}
